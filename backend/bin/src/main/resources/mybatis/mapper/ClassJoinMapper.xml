<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kb.samoim.dao.ClassJoinDao">
	<insert id="insertClassJoin" parameterType="com.kb.samoim.model.ClassJoin">
	    insert into CLASS_JOIN (
	    	CLASS_ID,
	    	USER_ID
	    ) values (
	    	#{classId}, 
	    	#{userId}
	    )
	</insert>
</mapper>
